#!/usr/bin/env bash

# /!\ Warning
# This file is generated by Kriollo, do not edit it manually 

if [[ $DEBUG ]]; then RUN_OPTS="-agentlib:jdwp=transport=dt_socket,server=y,suspend=y,address=*:5005"; fi;
if [[ $1 == 'self-update' ]]; then sh <(curl -L https://gitlab.com/mbarberot/kriollo/-/raw/main/src/main/bash/self-update.sh); exit 0; fi

java $RUN_OPTS -jar codegen/kriollo.jar $*

exit 0